<%- include('../_partials/breadcrumb') %>
    <section id="mainContent">
        <div class="container " style="padding-bottom: 30px;">
            <div class=" row ">
                <%- include('../_partials/setting') %>
                    <div class="col-12 col-sm-12 col-md-12 col-lg-9 ">
                        <form class="mt-3 ">
                            <div class="d-flex justify-content-between ">
                                <div class="d-flex profile-container ">
                                    <div class="profile-img mr-4 ">
                                        <img src="https://revhaut.lappy.ng/admin-dashboard/assets/images/icons/avatar.png " class="img-fluid rounded-circle ">
                                    </div>
                                    <div class="profile-content ">
                                        <h3>Kufre Ekong</h2>
                                            <a href=" ">Go to profile</a>
                                    </div>
                                </div>
                                <a class="edit " href="# ">Edit</a>
                            </div>


                            <div class="form-group ">
                                <label for="nameLabel "> Legal Name</label>
                                <input type="text " id="nameLabel " name=" " class="form-control " placeholder="Kufre Ekong ">
                            </div>
                            <div class="form-group ">
                                <label for="emailLabel "> Email Address</label>
                                <input type="email " id="emailLabel " name=" " class="form-control " placeholder="flekumxy@gmail.com ">
                            </div>
                            <div class="form-group ">
                                <label for="phoneLabel "> Phone Number</label>
                                <input type="text " id="phoneLabel " name=" " class="form-control " placeholder="+234 7035345753 ">
                            </div>
                            <div class="row ">
                                <div class="form-group col-6 mr-9 ">
                                    <label for="Country ">Country</label>
                                    <select class=" form-control " name=" " id=" ">
                  <option  value="Nigeria ">Nigeria</option>
                </select>
                                </div>
                                <div class="form-group col-6 ">
                                    <label for="state ">State</label>
                                    <select class="form-control " name="State " id=" ">
                  <option  value="Akwa Ibom ">Akwa Ibom</option>
                </select>
                                </div>
                            </div>
                            <div class="d-flex justify-content-between ">
                                <a class="edit " href="# ">Cancel</a>
                                <input type="button " value="Save " class="btn btn-secondary " disabled>
                            </div>
                        </form>

                    </div>
            </div>
        </div>
    </section>